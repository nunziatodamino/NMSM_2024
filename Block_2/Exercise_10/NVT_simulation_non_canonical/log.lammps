# 1) System initialization

units       lj
boundary    p p p
atom_style  atomic

variable box_size           equal 10
variable volume             equal ${box_size}*${box_size}*${box_size}
variable volume             equal 10*${box_size}*${box_size}
variable volume             equal 10*10*${box_size}
variable volume             equal 10*10*10

# 1) NVT ensemble simulation with velocity rescaling

variable step               equal 0.025
variable nsteps             equal 5

label loop_start
variable index loop         1 ${nsteps}
variable index loop         1 5
variable numberDensity      equal 0.2-${index}*${step}
